<!-- Modal -->
<div class="modal fade" id="modalInteresses-ensino" tabindex="-1" role="dialog" aria-labelledby="modalInteresses"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalInteressesTitulo">Selecione seus interesses</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form action="">
          <input type="hidden" name="tipo" value="ensino">
          <input list="interesses-list" name="input-interesse" id="input-interesse"
            class="custom-select custom-select-lg mb-3" placeholder="#javascript">
          <datalist id="interesses-list">
              <option value="Edge">
              <option value="Firefox">
              <option value="Chrome">
              <option value="Opera">
              <option value="Safari">
          </datalist>
        <button type="button" class="btn btn-success" id="adicionar_interesse_ap">Adicionar</button>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
        <button type="button" class="btn btn-primary">Salvar</button>
      </div>
    </div>
  </div>
</div>



