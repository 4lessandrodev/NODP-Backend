<%if(errors){ %>
  <div <div class="alert alert-warning alert-dismissible fade show" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
    <ul>
      <% for(let erro of errors){ %>
        <li><%=erro.msg%></li>
        <% } %> 
      </ul>
    </div>
    <%}%> 

